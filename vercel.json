{
  "outputDirectory": ".",
  "cleanUrls": true,
  "redirects": [
    {
      "source": "/auto-clip-maker",
      "destination": "/free-trial",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/free-trial",
      "destination": "/signup1"
    },
    {
      "source": "/api/clips/:path*",
      "destination": "https://api.autostreampro.com/api/clips/:path*"
    },
    {
      "source": "/api/process/:path*",
      "destination": "https://api.autostreampro.com/api/process/:path*"
    },
    {
      "source": "/api/ai/:path*",
      "destination": "https://api.autostreampro.com/api/ai/:path*"
    },
    {
      "source": "/api/tiktok-creator-info",
      "destination": "https://api.autostreampro.com/api/tiktok-creator-info"
    },
    {
      "source": "/api/upload-to-tiktok",
      "destination": "https://api.autostreampro.com/api/upload-to-tiktok"
    }
  ],
  "headers": [
    {
      "source": "/api/:path*",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,POST,OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ]
}