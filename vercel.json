{
  "buildCommand": "echo 'No build needed'",
  "outputDirectory": ".",
  "framework": null,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    
    {
      "source": "/auth/youtube/callback",
      "destination": "https://autostreampro-backend-production.up.railway.app/auth/youtube/callback"
    }, 
    
    {
      "source": "/auth/youtube",
      "destination": "https://autostreampro-backend-production.up.railway.app/auth/youtube"
    },


    {
      "source": "/api/:path*",
      "destination": "https://autostreampro-backend-production.up.railway.app/api/:path*"
    },
    {
      "source": "/api/health",
      "destination": "https://autostreampro-backend-production.up.railway.app/health"
    } 

    
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "autostreampro.com"
        }
      ],
      "destination": "https://www.autostreampro.com/$1",
      "permanent": true
    }
  ]
}
